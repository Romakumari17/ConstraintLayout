        android:layout_width="0dp"
        android:layout_height="80dp"
        android:background="#B58DBC"
        android:src="@drawable/baseline_smart_button_24"
        app:layout_constraintBottom_toBottomOf="@id/guide_.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/guide_.75"
        app:layout_constraintEnd_toEndOf="@id/guide_.47"/>
    <ImageView
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:background="#B58DBC"
        android:src="@drawable/baseline_cloud_24"
        app:layout_constraintBottom_toBottomOf="@id/guide_.72"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/guide_.47"
        app:layout_constraintTop_toTopOf="@id/guide_.5"
        android:id="@+id/view5"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/guide_.2.3"
        app:layout_constraintGuide_percent=".23"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/guide_.73"
        app:layout_constraintGuide_percent=".73"
        />
    <androidx.constraintlayout.widget.Guideline
